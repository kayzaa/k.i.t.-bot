{
  "name": "kit-trading",
  "version": "2.0.0",
  "description": "K.I.T. - Knight Industries Trading: An AI Agent Framework for Financial Markets",
  "keywords": [
    "trading",
    "ai",
    "agent",
    "framework",
    "crypto",
    "forex",
    "technical-analysis",
    "automated-trading"
  ],
  "author": "K.I.T. Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayzaa/k.i.t.-bot.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "kit": "./dist/src/cli/kit.js",
    "kit-trading": "./scripts/install.js"
  },
  "files": [
    "dist",
    "skills",
    "scripts",
    "workspace-templates"
  ],
  "scripts": {
    "postinstall": "node scripts/setup.js || true",
    "setup": "node scripts/setup.js",
    "build": "tsc && npm run copy-hooks",
    "copy-hooks": "node -e \"const fs=require('fs');const path=require('path');const src='src/hooks/bundled';const dst='dist/src/hooks/bundled';if(fs.existsSync(src)){fs.readdirSync(src).forEach(d=>{const hookMd=path.join(src,d,'HOOK.md');const dstMd=path.join(dst,d,'HOOK.md');if(fs.existsSync(hookMd)){fs.mkdirSync(path.dirname(dstMd),{recursive:true});fs.copyFileSync(hookMd,dstMd);}});}\"",
    "dev": "ts-node src/index.ts",
    "start": "node dist/src/index.js",
    "cli": "node dist/cli/kit.js",
    "install-kit": "node scripts/install.js",
    "gateway": "ts-node src/gateway/server.ts",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@clack/prompts": "^1.0.0",
    "@google/generative-ai": "^0.8.0",
    "@slack/bolt": "^3.17.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "axios": "^1.6.0",
    "ccxt": "^4.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "discord.js": "^14.14.0",
    "dotenv": "^16.4.0",
    "ethers": "^6.16.0",
    "eventemitter3": "^5.0.0",
    "express": "^4.18.0",
    "fastify": "^5.7.4",
    "form-data": "^4.0.5",
    "node-cron": "^4.2.1",
    "ollama": "^0.5.0",
    "open": "^10.0.0",
    "openai": "^4.47.0",
    "ora": "^8.0.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.7.0",
    "technicalindicators": "^3.1.0",
    "telegraf": "^4.16.0",
    "tmi.js": "^1.8.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.19.33",
    "@types/node-cron": "^3.0.11",
    "@types/tmi.js": "^1.8.0",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.0",
    "playwright": "^1.58.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
