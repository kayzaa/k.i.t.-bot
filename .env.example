# K.I.T. Environment Variables
# Copy this file to .env and fill in your values

# ================================================
# AI Providers (at least one required)
# ================================================

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-...

# OpenAI
# OPENAI_API_KEY=sk-...

# OpenRouter (access multiple models)
# OPENROUTER_API_KEY=sk-or-...

# Google (Gemini)
# GOOGLE_API_KEY=...

# Groq (fast inference)
# GROQ_API_KEY=gsk_...

# ================================================
# Gateway Settings
# ================================================

KIT_GATEWAY_HOST=127.0.0.1
KIT_GATEWAY_PORT=18799
# KIT_GATEWAY_TOKEN=your-secret-token

# ================================================
# Crypto Exchanges
# ================================================

# Binance
# BINANCE_API_KEY=...
# BINANCE_SECRET=...
# BINANCE_SANDBOX=true

# Bybit
# BYBIT_API_KEY=...
# BYBIT_SECRET=...

# Kraken
# KRAKEN_API_KEY=...
# KRAKEN_SECRET=...

# Coinbase
# COINBASE_API_KEY=...
# COINBASE_SECRET=...
# COINBASE_PASSPHRASE=...

# ================================================
# MT5 (Windows Only)
# ================================================

# MT5_SERVER=MetaQuotes-Demo
# MT5_LOGIN=12345678
# MT5_PASSWORD=your-password

# ================================================
# Messaging Channels
# ================================================

# Telegram
TELEGRAM_BOT_TOKEN=...
TELEGRAM_CHAT_ID=...

# Discord
# DISCORD_BOT_TOKEN=...
# DISCORD_CHANNEL_ID=...

# Slack
# SLACK_BOT_TOKEN=xoxb-...
# SLACK_CHANNEL_ID=...

# ================================================
# Trading Settings
# ================================================

KIT_TRADING_ENABLED=false
KIT_AUTO_TRADE=false

# ================================================
# Logging
# ================================================

KIT_LOG_LEVEL=info

# ================================================
# Memory/Embeddings
# ================================================

# For semantic search (optional)
# PINECONE_API_KEY=...
# PINECONE_ENVIRONMENT=...
